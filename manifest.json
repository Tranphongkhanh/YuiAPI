{
    "manifest_version": 2,
    "name": "YuiAPI - A REST Client, Api Test and document",
    "version": "0.0.5",
    "description":"REST Client, Api Test and document",
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "background": {
        "scripts": [
            "scripts/background.js"
        ]
    },
    "browser_action": {
        "default_title": "YuiAPI RESTClient Test Document",
        "default_icon": "images/logo48.png"
    },
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "icons": {
        "16": "images/logo16.png",
        "48": "images/logo48.png",
        "128": "images/logo128.png"
    }
}