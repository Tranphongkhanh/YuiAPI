{
    "manifest_version": 2,
    "name": "YuiAPI",
    "version": "0.0.1",
    "background": {
        "scripts": [
            "scripts/background.js"
        ]
    },
    "browser_action": {
        "default_title": "YuiAPI"
    },
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ]
}